{
  "name": "VirtualPingPong",
  "version": "0.0.1",
  "main": "server.js",
  "scripts": {
    "npi": "node server.js",
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "clean": "rm -rf dist/js/*",
    "serve": "http-server . -p 5556",
    "build": "npm run clean && tsc",
    "build_prod": "npm run build && browserify -s main build/main.js > dist/js/bundle.js && npm run minify",
    "minify": "uglifyjs build/full-build.js --screw-ie8 --compress --mangle --output dist/js/bundle.min.js"
  },
  "dependencies": {
    "@angular/common": "~2.1.0",
    "@angular/compiler": "~2.1.0",
    "@angular/core": "~2.1.0",
    "@angular/forms": "~2.1.0",
    "@angular/http": "~2.1.0",
    "@angular/platform-browser": "~2.1.0",
    "@angular/platform-browser-dynamic": "~2.1.0",
    "@angular/router": "~3.1.0",
    "@angular/upgrade": "~2.1.0",
    "@types/core-js": "^0.9.34",
    "@types/socket.io-client": "^1.4.29",
    "angular2-google-maps": "^0.16.0",
    "angular2-uuid": "^1.1.0",
    "body-parser": "^1.15.2",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "mongoose": "^4.7.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "shake.js": "^1.2.2",
    "socket.io": "^1.5.1",
    "systemjs": "0.19.39",
    "typescript": "^2.0.3",
    "typings": "^1.4.0",
    "uuid-random": "^1.0.5",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-minify": "0.0.14",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-streamify": "^1.0.2",
    "gulp-typescript": "^3.1.2",
    "gulp-uglify": "^2.0.0",
    "run-sequence": "^1.2.2",
    "vinyl-source-stream": "^1.1.0"
  }
}
